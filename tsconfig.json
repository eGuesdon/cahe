{
    "compilerOptions":{
        "target": "es5",
        "module": "commonjs",
        "declaration": true,
        "declarationDir": "./typings/cahe/",
        "removeComments": true,
        "moduleResolution": "classic",
        "watch": true,
        "outDir":"./out",
        "sourceMap": false
    },

    "include":[
        "./src/**/*.ts","./src/**/*.css"
    ],

    "exclude": [
        "./out/**/*", "./node_modules", "./typings/**/*.d.ts"
    ]
}